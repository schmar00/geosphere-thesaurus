<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/56">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
PREFIX dbpo:&amp;lt;http://dbpedia.org/ontology/&amp;gt;
SELECT ?Label ?color ?r ?g ?b ?uri
WHERE {
?uri a skos:Concept; skos:prefLabel ?Label; 
dbpo:colourHexCode ?c . FILTER(lang(?Label)="de") .
BIND(CONCAT("\u003Cspan style='background-color:",?c,
";'\u003E",?c,"\u003C\u002Fspan\u003E") AS ?color)

BIND(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE
(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE
(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE
(?c,"0","00"),"9","09"),"8","08"),
"7","07"),"6","06"),"5","05"),"4","04"),
"3","03"),"2","02"),"1","01"),"F","15"),
"E","14"),"D","13"),"C","12"),"B","11"),
"A","10") AS ?F) .
BIND(xsd:integer(SUBSTR(?F,2,2))*16 + 
xsd:integer(SUBSTR(?F,4,2)) AS ?r) 
BIND(xsd:integer(SUBSTR(?F,6,2))*16 + 
xsd:integer(SUBSTR(?F,8,2)) AS ?g) 
BIND(xsd:integer(SUBSTR(?F,10,2))*16 + 
xsd:integer(SUBSTR(?F,12,2)) AS ?b) 
}
ORDER BY ?Label
&lt;/pre&gt;
e.g. try &amp;nbsp;&amp;nbsp;&lt;a href="https://resource.geolba.ac.at/PoolParty/sparql/lithology"&gt;https://resource.geolba.ac.at/PoolParty/sparql/lithology&lt;/a&gt;</skos:definition>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-01T06:56:22.659Z</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/50">
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-01T11:20:34.088Z</dcterms:modified>
	<skos:definition xml:lang="de">Per Email an das Thesaurus-Redaktionsteam der Geologische Bundesanstalt - &lt;a href="mailto:thesaurus@geologie.ac.at"&gt;thesaurus@geologie.ac.at&lt;/a&gt;. &lt;br&gt;Zusätzlich kann über den Tab "Feedback" im Navigationsbereich rechts oben ein Thesauruskonzept direkt per Email an das Thesaurus-Redaktionsteam kommentiert werden.</skos:definition>
