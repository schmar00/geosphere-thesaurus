	<skos:definition xml:lang="de">PREFIX dc:&lt;http://purl.org/dc/elements/1.1/&gt;&amp;#xa;PREFIX n0pred: &lt;http://reegle.info/schema#&gt;&amp;#xa;SELECT ?project ?title ?mainActivities ?expectedImpact ?purpose ?background ?budget&amp;#xa;WHERE &amp;#xa;{&amp;#xa; ?project a &lt;http://reegle.info/schema#Project&gt; . &amp;#xa;?project dc:title ?title;&amp;#xa;n0pred:mainActivities ?mainActivities;&amp;#xa;n0pred:expectedImpact ?expectedImpact;&amp;#xa;n0pred:budget ?budget;&amp;#xa;n0pred:purpose ?purpose . FILTER regex(str(?mainActivities), 'geothermal')&amp;#xa;OPTIONAL {?project n0pred:background ?background}&amp;#xa;} </skos:definition>
