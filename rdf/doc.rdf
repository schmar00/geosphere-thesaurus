</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/164">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
PREFIX geo:&amp;lt;http://www.w3.org/2003/01/geo/wgs84_pos#&amp;gt;
SELECT 
?URI ?prefLabelDE ?lat_long
