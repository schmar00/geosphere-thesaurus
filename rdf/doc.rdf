<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/161">
	<skos:prefLabel xml:lang="de">Alle untergeordneten Begriffe von einem Begriff mit Beschreibungen</skos:prefLabel>
	<skos:prefLabel xml:lang="en">All narrower concepts plus descriptions</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/164">
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-09-27T13:39:28.794Z</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/161">
	<skos:definition xml:lang="de">E.g. replace [insert URI] with concept URI http://resource.geolba.ac.at/lithology/6&lt;br&gt;&lt;br&gt;
&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
SELECT *
WHERE {
&amp;lt;[insert URI]&amp;gt; skos:narrower* ?s . 
?s skos:prefLabel ?Label . FILTER(lang(?Label)='en')
OPTIONAL {?s skos:definition ?Definition .
FILTER(lang(?Definition)='en')}
}
&lt;/pre&gt;
and try &amp;nbsp;&amp;nbsp;&lt;a href="https://resource.geolba.ac.at/PoolParty/sparql/lithology"&gt;https://resource.geolba.ac.at/PoolParty/sparql/lithology&lt;/a&amp;&gt;</skos:definition>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-09-27T13:33:09.063Z</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/160">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
PREFIX dcterms:&amp;lt;http://purl.org/dc/terms/&amp;gt;
SELECT ?URI ?prefLabelDE ?date
WHERE
{
?URI a skos:Concept . ?URI skos:prefLabel ?prefLabelDE . 
FILTER(lang(?prefLabelDE)='de') . ?URI dcterms:modified ?date
}
ORDER BY DESC (?date)
LIMIT 10
&lt;/pre&gt;
e.g. try &amp;nbsp;&amp;nbsp;&lt;a href="https://resource.geolba.ac.at/PoolParty/sparql/lithology"&gt;https://resource.geolba.ac.at/PoolParty/sparql/lithology&lt;/a&amp;&gt;</skos:definition>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-09-27T13:37:31.584Z</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/167">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX dcterms:&amp;lt;http://purl.org/dc/terms/&amp;gt;
PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
PREFIX geo:&amp;lt;http://www.w3.org/2003/01/geo/wgs84_pos#&amp;gt;
PREFIX dbpo:&amp;lt;http://dbpedia.org/ontology/&amp;gt;
PREFIX gba:&amp;lt;http://resource.geolba.ac.at/schema/thes#&amp;gt;

SELECT ?URI 
(MIN(?prefLabelDE) AS ?PrefLabelDe) (MIN(?prefLabelEN) AS ?PrefLabelEn)
(MIN(?descDE) AS ?DescriptionDe) (MIN(?descEN) AS ?DescriptionEn)
(GROUP_CONCAT(DISTINCT ?tc; separator = '; ') as ?TopConceptURI)
(GROUP_CONCAT(DISTINCT ?tcpDE; separator = '; ') as ?TopConceptPrefLabelDe)
(GROUP_CONCAT(DISTINCT ?tcpEN; separator = '; ') as ?TopConceptPrefLabelEn)
(GROUP_CONCAT(DISTINCT ?cs; separator = '; ') as ?ConceptSchemeURI)
(GROUP_CONCAT(DISTINCT ?cspDE; separator = '; ') as ?ConceptSchemeTitleDe)
(GROUP_CONCAT(DISTINCT ?cspEN; separator = '; ') as ?ConceptSchemeTitleEn)
(MIN(?color) AS ?HexColor) (MIN(?status) AS ?GBA_Status) 
(MIN(?lat_long) AS ?Pos_Lat_Long)
(GROUP_CONCAT(DISTINCT ?broader; separator = '; ') as ?BroaderConceptURI)
(GROUP_CONCAT(DISTINCT ?broaderDE; separator = '; ') as ?BroaderConceptPrefLabelDe)
(GROUP_CONCAT(DISTINCT ?broaderEN; separator = '; ') as ?BroaderConceptPrefLabelEn)

(MIN(?inspire) AS ?InspireMappingURI) 
(MIN(?dbp) AS ?DBpediaMappingURI) 
(MIN(?gsml) AS ?GeoscimlMappingURI) 
(MIN(?bgs) AS ?BgsMappingURI) 
(MIN(?notation) AS ?Notation)
(MIN(?DC) AS ?DateCreated) 
(MIN(?DM) AS ?DateModified)

WHERE
{
?URI a skos:Concept .
FILTER (regex(str(?URI), '^http://resource.geolba.ac.at/.'))  
OPTIONAL {?URI skos:prefLabel ?prefLabelDE . FILTER(lang(?prefLabelDE)='de')} .
OPTIONAL {?URI skos:prefLabel ?prefLabelEN . FILTER(lang(?prefLabelEN)='en')} .
OPTIONAL {?URI skos:definition ?descDE . FILTER(lang(?descDE)='de')} .
OPTIONAL {?URI skos:definition ?descEN . FILTER(lang(?descEN)='en')} .
OPTIONAL {?URI dbpo:colourHexCode ?color} .
OPTIONAL {?URI gba:status ?status} .
OPTIONAL {?URI geo:lat_long ?lat_long} .
OPTIONAL {?URI skos:broader ?broader . ?broader skos:prefLabel ?broaderDE .
FILTER(lang(?broaderDE)='de')} . 
OPTIONAL {?URI skos:broader ?broader . ?broader skos:prefLabel ?broaderEN .
FILTER(lang(?broaderEN)='en')} .

OPTIONAL {
{?URI skos:exactMatch ?inspire}
UNION
{?URI skos:closeMatch ?inspire}
UNION
{?URI skos:broadMatch ?inspire}
UNION
{?URI skos:narrowMatch ?inspire} .
FILTER regex(str(?inspire), '^http://inspire.ec.europa.eu')} .

OPTIONAL {
{?URI skos:exactMatch ?dbp} UNION {?URI skos:closeMatch ?dbp}
UNION
{?URI skos:broadMatch ?dbp} UNION {?URI skos:narrowMatch ?dbp} .
FILTER regex(str(?dbp), '^http://dbpedia.org')} .

OPTIONAL {
{?URI skos:exactMatch ?gsml} UNION {?URI skos:closeMatch ?gsml}
UNION
{?URI skos:broadMatch ?gsml} UNION {?URI skos:narrowMatch ?gsml} .
FILTER regex(str(?gsml), '^http://resource.geosciml.org')} .

OPTIONAL {
{?URI skos:exactMatch ?bgs} UNION {?URI skos:closeMatch ?bgs}
UNION
{?URI skos:broadMatch ?bgs} UNION {?URI skos:narrowMatch ?bgs} .
FILTER regex(str(?bgs), '^http://data.bgs.ac.uk')} .

OPTIONAL {?URI skos:notation ?notation} .
OPTIONAL {?URI dcterms:created  ?DC} .
OPTIONAL {?URI dcterms:modified  ?DM} .
OPTIONAL {?URI skos:broader* ?tc . ?cs skos:hasTopConcept ?tc .
?cs dcterms:title ?cspDE . FILTER(lang(?cspDE)='de') . 
?cs dcterms:title ?cspEN . FILTER(lang(?cspEN)='en') .
?tc skos:prefLabel ?tcpDE . FILTER(lang(?tcpDE)='de') . 
?tc skos:prefLabel ?tcpEN . FILTER(lang(?tcpEN)='en')}

}
GROUP BY ?URI
ORDER BY ?URI
&lt;/pre&gt;
e.g. try &amp;nbsp;&amp;nbsp;&lt;a href="https://resource.geolba.ac.at/PoolParty/sparql/lithology"&gt;https://resource.geolba.ac.at/PoolParty/sparql/lithology&lt;/a&amp;&gt;</skos:definition>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-09-27T13:37:54.992Z</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/162">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
SELECT ?URI ?prefLabelDE ?definitionDE ?prefLabelEN ?definitionEN 
WHERE {
?URI a skos:Concept ; skos:prefLabel ?prefLabelDE .
FILTER(lang(?prefLabelDE)='de') .
FILTER (!strStarts(?prefLabelDE, '['^^xsd:string)) .
OPTIONAL{?URI skos:prefLabel ?prefLabelEN .
FILTER(lang(?prefLabelEN)='en')}
OPTIONAL{?URI skos:definition ?definitionDE .
FILTER(lang(?definitionDE)='de')} 
OPTIONAL{?URI skos:definition ?definitionEN .
FILTER(lang(?definitionEN)='en')}
} 
ORDER BY ?prefLabelDE
&lt;/pre&gt;
e.g. try &amp;nbsp;&amp;nbsp;&lt;a href="https://resource.geolba.ac.at/PoolParty/sparql/GeologicUnit"&gt;https://resource.geolba.ac.at/PoolParty/sparql/GeologicUnit&lt;/a&amp;&gt;</skos:definition>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-09-27T13:38:14.085Z</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/163">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
PREFIX gba:&amp;lt;http://resource.geolba.ac.at/schema/thes#&amp;gt;
SELECT ?URI ?prefLabelDE
WHERE
{
?URI a skos:Concept . ?URI skos:prefLabel ?prefLabelDE . 
FILTER(lang(?prefLabelDE)='de') .
?URI gba:status ?status . 
FILTER regex(str(?status), '1')
}
ORDER BY ?prefLabelDE
&lt;/pre&gt;
e.g. try &amp;nbsp;&amp;nbsp;&lt;a href="https://resource.geolba.ac.at/PoolParty/sparql/GeologicUnit"&gt;https://resource.geolba.ac.at/PoolParty/sparql/GeologicUnit&lt;/a&amp;&gt;</skos:definition>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-09-27T13:38:46.662Z</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/166">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
PREFIX foaf:&amp;lt;http://xmlns.com/foaf/0.1/&amp;gt;
SELECT ?s ?Label ?picture
WHERE {
?s foaf:depiction ?picture .
?s skos:prefLabel ?Label . FILTER(lang(?Label)='en')
}
