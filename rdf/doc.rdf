<rdf:Description rdf:about="http://resource.geolba.ac.at/doc/165">
	<skos:definition xml:lang="de">&lt;pre&gt;PREFIX skos:&amp;lt;http://www.w3.org/2004/02/skos/core#&amp;gt;
SELECT ?URI ?prefLabel ?desc ?inspire ?gsml
WHERE {
?URI a skos:Concept . ?URI skos:prefLabel ?prefLabel . 
FILTER(lang(?prefLabel)='en') .
OPTIONAL {?URI skos:definition ?desc . 
FILTER(lang(?desc)='en')} .
OPTIONAL {?URI ?p1 ?inspire . 
FILTER (regex(str(?inspire), '^http://inspire.ec.europa.eu'))}
 OPTIONAL {?URI ?p2 ?gsml . 
FILTER (regex(str(?gsml), '^http://resource.geosciml.org'))} 
FILTER (BOUND(?inspire) &amp;vert;&amp;vert; BOUND(?gsml))
}
ORDER BY ?prefLabel
&lt;/pre&gt;
e.g. try &amp;nbsp;&amp;nbsp;&lt;a href="https://resource.geolba.ac.at/PoolParty/sparql/lithology"&gt;https://resource.geolba.ac.at/PoolParty/sparql/lithology&lt;/a&amp;&gt;</skos:definition>
