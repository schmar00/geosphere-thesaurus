	<skos:definition xml:lang="de">PREFIX skos:&lt;http://www.w3.org/2004/02/skos/core#&gt;&amp;#xa;PREFIX gba:&lt;http://resource.geolba.ac.at/PoolParty/schema/GBA/&gt;&amp;#xa;SELECT DISTINCT ?URI (STR(?Lde) AS ?Label_de) (STR(?Len) AS ?Label_en) (STR(?Dde) AS ?Def_de) (STR(?Den) AS ?Def_en)&amp;#xa; (STR(?Notede) AS ?Note_de) (STR(?Noteen) AS ?Note_en) (STR(?topLde) AS ?topLabel_de) ?GBA_Status&amp;#xa;WHERE{VALUES ?CS {&lt;http://resource.geolba.ac.at/GeologicUnit/235&gt; &lt;http://resource.geolba.ac.at/GeologicUnit/1&gt;} &amp;#xa;?CS skos:hasTopConcept ?topURI . ?topURI skos:narrower* ?URI; skos:prefLabel ?topLde .&amp;#xa;FILTER(lang(?topLde)='de') . &amp;#xa;?URI skos:prefLabel ?Lde; skos:prefLabel ?Len; skos:definition ?Dde; skos:definition ?Den .&amp;#xa;OPTIONAL {?URI skos:scopeNote ?Notede ; skos:scopeNote ?Noteen . FILTER(lang(?Notede)='de') . FILTER(lang(?Noteen)='en')}&amp;#xa;OPTIONAL {?URI gba:GBA_Status ?GBA_Status}&amp;#xa;FILTER(lang(?Lde)='de') . FILTER(lang(?Len)='en') . FILTER(lang(?Dde)='de') . FILTER(lang(?Den)='en') }</skos:definition>
</rdf:Description>

