<rdf:Description rdf:about="http://resource.geolba.ac.at/minres/102">
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-05-28T07:16:37.041Z</dcterms:created>
	<skos:broader rdf:resource="http://resource.geolba.ac.at/minres/16"/>
